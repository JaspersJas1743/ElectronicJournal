<UserControl x:Class="ElectronicJournal.Resources.UserControls.SvgWithChangeOfTheme"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ElectronicJournal.Resources.UserControls"
             xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
             mc:Ignorable="d" x:Name="ThisElement" d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <svgc:SvgViewbox x:Name="SvgPresenter" Source="{Binding ElementName=ThisElement,Path=SvgFileName}"/>
    </Grid>
</UserControl>
