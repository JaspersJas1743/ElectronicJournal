<UserControl x:Class="ElectronicJournal.Views.Registration"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:vm="clr-namespace:ElectronicJournal.ViewModels" 
      xmlns:local="clr-namespace:ElectronicJournal.Views" mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Viewbox Grid.Row="0">
            <TextBlock Text="Регистрация"/>
        </Viewbox>
        <Button Grid.Row="1" Content="Назад" Command="{Binding Back}"/>
    </Grid>
</UserControl>
